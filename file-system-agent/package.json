{
  "name": "file-system-agent",
  "version": "1.0.0",
  "description": "Agent local pour l'accès au système de fichiers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "pkg": "npm run build && pkg dist/index.js --targets node18-linux-x64,node18-win-x64,node18-macos-x64 --out-path ./bin"
  },
  "keywords": [
    "filesystem",
    "agent",
    "docker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@types/chokidar": "^2.1.7",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "fastify": "^4.24.3",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
